<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewHistoryTransactionActivity">

    <TextView
        android:id="@+id/viewHistoryAmount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="400"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/creationDateLabel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transaction Created:"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="327dp" />

    <TextView
        android:id="@+id/viewHistoryDirection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="YOU O:"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/viewHistoryCreationDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Oct 4, 2021"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="250dp"
        tools:layout_editor_absoluteY="327dp" />

    <TextView
        android:id="@+id/viewHistoryPerson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jeffrey"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="186dp" />

    <TextView
        android:id="@+id/pesoLabel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PHP"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/viewTransactionLabel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Transaction"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.139" />

    <TextView
        android:id="@+id/viewHistoryContactDetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0917-xxx-xxxx"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="273dp" />

    <TextView
        android:id="@+id/contactDetailsLabel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact Details: "
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="273dp" />
</androidx.constraintlayout.widget.ConstraintLayout>